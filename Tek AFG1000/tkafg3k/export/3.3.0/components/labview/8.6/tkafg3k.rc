	"Standard Function Output:Waveform"
	iL enum { TKAFG3K_VAL_WFM_SINE=1, TKAFG3K_VAL_WFM_SQUARE=2, TKAFG3K_VAL_WFM_DC=6, TKAFG3K_VAL_WFM_RAMP=1001, TKAFG3K_VAL_WFM_PULS=1002, TKAFG3K_VAL_WFM_PRN=1003, TKAFG3K_VAL_WFM_SINC=1004, TKAFG3K_VAL_WFM_GAUS=1005, TKAFG3K_VAL_WFM_LOR=1006, TKAFG3K_VAL_WFM_ERIS=1007, TKAFG3K_VAL_WFM_EDEC=1008, TKAFG3K_VAL_WFM_HAV=1009 } readwrite local 131335
	//const: TKAFG3K_ATTR_FUNC_WAVEFORM
